<?xml version="1.0" ?>

<robot name="artag">
  
  <link name="artag">
    <visual>
      <geometry>
        <mesh filename="file://$(find x4_description)/meshes/artag.dae" />
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="file://$(find x4_description)/meshes/artag.dae" />
      </geometry>
    </collision>
    <inertial>
      <mass value="1" />
      <inertia ixx="1" ixy="0.0" ixz="0.0"
               iyy="1" iyz="0.0"
               izz="1" />
    </inertial>
  </link>

  <link name="world"/>
  <joint name="artag_joint" type="fixed">
    <axis xyz="0 0 1" />
    <origin xyz="0.5 0 0.0" rpy="0 0 0"/>
    <parent link="world"/>
    <child link="artag"/>
  </joint>
  
</robot>
